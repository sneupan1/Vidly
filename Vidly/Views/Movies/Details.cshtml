@model Vidly.Models.Movie 
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.MovieGenre.Name</li>
    <li>Release Date: @Model.ReleaseDate</li>
    <li>Data Added: @Model.DateAdded</li>
    <li>Number in Stocks: @Model.NumberInStock</li>
</ul>

